    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css">
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@500&display=swap');
        * {
            padding: 0;
            margin: 0;
            box-sizing: border-box;
            font-size: 100%;
            border: 0;
            font-family: 'Poppins', sans-serif;
        }
        
        .wapper {
            min-height: 100vh;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        
        .control {
            display: flex;
            justify-content: space-between;
            width: 500px
        }
        
        .control .btn{
            width: 120px;
            text-align: center;
            border-radius: 10px;
        }
        
        .control .btn.btn-success {
            background-color: greenyellow
        }
        
        
        .control .btn.btn-error {
            background-color: rgb(255, 89, 47)
        }
        
        .control .btn.btn-info {
            background-color: blue
        }
        
        
        #toast {
            position: fixed;
            top: 0;
            right: 0;
            /* background-color: antiquewhite; */
            width: 400px;
            z-index: 9999;
        }
        
        #toast .notification {
            background-color:white ;
            height: 90px;
            margin: 20px 10px;
            border-radius: 5px;
            display: flex;
            box-shadow: 0 0 5px rgb(143, 142, 142);
            position: relative;
            animation: toast-in ease .3s,toast-out ease .5s 3s forwards;
            transition:all 0.5s ease;
        }
        
        @keyframes toast-in {
            from {
                transform: translateX(calc(100% + 10px))
            }
            to {
                transform: translateX(0)
            }
        }
        
        @keyframes toast-out {
            from {
                opacity: 1
            }
            to {
                opacity: 0
            }
        }
        
        #toast .notification.success {
            border-left: 4px solid greenyellow;
        }
        
        #toast .notification.error {
            border-left: 4px solid rgb(255, 89, 47);
        }
        
        #toast .notification.info {
            border-left: 4px solid rgb(91, 91, 184);
        }
        
        #toast .notification .icon {
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 20px;
            width: 40px;
        }
        
        #toast .notification.success .icon {
            color: greenyellow;
        }
        
        #toast .notification.error .icon {
            color: rgb(255, 89, 47);
        }
        
        #toast .notification.info .icon {
            color: rgb(91, 91, 184);
        }
        
        #toast .notification .noti {
            margin: auto 0;
            width: calc(100% - 40px - 40px);
        }
        
        #toast .notification .noti .title {
            font-size: 18px;
            font-weight: 600;
            margin-bottom: 3px;
        }
        
        #toast .notification .noti .message {
            color: rgb(167, 169, 170);
            font-size: 13px;
            width: calc(100%);
            max-height: 40px;
            overflow-y: hidden;
            
        }
        
        #toast .notification .close {
            position: absolute;
            right: 0px;
            width: 40px;
            text-align: center;
            line-height: 90px;
            color: rgb(167, 169, 170);
            font-size: 18px;
        }
        
    </style>
    <div id="toast">
    </div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>check</th>
                <th>Id đơn hàng</th>
                <th>Ngày đặt hàng</th>
                <th>Trạng thái</th>
                <th>Lượng mặt hàng</th>
                <th>Giá trị đơn hàng</th>
                <th>Mã vận đơn</th>
                <th>Đơn vị vận chuyển</th>
                <th>Huỷ đơn</th>
            </tr>
        </thead>
        <tbody>
            <% for(var i = 0; i < order.length; i++) { 
                if(order[i].TinhTrangDonHang == 4) {%>
                    <tr style="background-color: aquamarine;">
                <% } else {%>
                    <tr>
                <% } %>
                    <th scope="row"><input type="checkbox" name="check" id=""></th>
                    <td><%= order[i].id %></td>
                    <td><%= order[i].ngayTao %></td>
                    <td><%= order[i].tinhTrang %></td>
                    <td><%= order[i].soLuong %></td> 
                    <td>20000vnd</td>
                    <td><%= order[i].maVanDon %></td>
                    <td><%= order[i].donViVanChuyen %></td>
                    <td>
                        <% if(order[i].TinhTrangDonHang != 4) {%>
                            <i class="fa-solid fa-trash trash" id="cancel_order"></i>
                        <% } %>
                    </td>
                </tr>
            <% } %>
        </tbody>
    </table>
